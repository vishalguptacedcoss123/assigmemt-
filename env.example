# Rudderstack Credentials
RUDDERSTACK_EMAIL=your-business-email@domain.com
RUDDERSTACK_PASSWORD=your-password

# Environment URLs
DEV_URL=https://app.rudderstack.com
QA_URL=https://app.rudderstack.com
PROD_URL=https://app.rudderstack.com

# Current Environment (dev, qa, prod)
CURRENT_ENV=dev

# API Configuration
API_TIMEOUT=30
API_RETRY_ATTEMPTS=3
API_BASE_URL=https://api.rudderstack.com

# Test Configuration
HEADLESS_MODE=true
BROWSER_TIMEOUT=10000
IMPLICIT_WAIT=10
PAGE_LOAD_TIMEOUT=30

# Browser Configuration
BROWSER_NAME=chrome
BROWSER_VERSION=latest
WINDOW_WIDTH=1920
WINDOW_HEIGHT=1080

# Webhook Configuration
WEBHOOK_URL=https://your-requestcatcher-url.requestcatcher.com
WEBHOOK_TIMEOUT=60

# Reporting Configuration
GENERATE_HTML_REPORT=true
GENERATE_ALLURE_REPORT=true
SCREENSHOT_ON_FAILURE=true
VIDEO_RECORDING=false

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=test_execution.log

# Parallel Execution
MAX_WORKERS=2
PARALLEL_MODE=false

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=2

# Notification Configuration
SLACK_WEBHOOK_URL=
EMAIL_NOTIFICATIONS=false
EMAIL_SMTP_SERVER=
EMAIL_SMTP_PORT=
EMAIL_USERNAME=
EMAIL_PASSWORD= 