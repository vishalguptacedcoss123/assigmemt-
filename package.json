{
  "name": "rudderstack-sdet-assignment",
  "version": "1.0.0",
  "description": "SDET Assignment Framework for Rudderstack Testing",
  "main": "index.js",
  "scripts": {
    "test": "wdio run config/wdio.conf.js",
    "test:parallel": "wdio run config/wdio.conf.js --parallel 2",
    "test:headless": "wdio run config/wdio.conf.js --headless",
    "test:chrome": "wdio run config/wdio.conf.js --browser chrome",
    "test:firefox": "wdio run config/wdio.conf.js --browser firefox",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:headed": "playwright test --headed",
    "install:browsers": "playwright install",
    "report": "allure generate allure-results --clean && allure open",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "webdriverio",
    "playwright",
    "selenium",
    "pytest",
    "rudderstack",
    "automation",
    "testing"
  ],
  "author": "SDET Candidate",
  "license": "MIT",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.32.5",
    "@wdio/appium-service": "^8.32.5",
    "@wdio/cli": "^8.32.5",
    "@wdio/local-runner": "^8.32.5",
    "@wdio/mocha-framework": "^8.32.5",
    "@wdio/spec-reporter": "^8.32.5",
    "@wdio/types": "^8.32.5",
    "@playwright/test": "^1.40.0",
    "allure-commandline": "^2.24.1",
    "chromedriver": "^119.0.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "geckodriver": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "wdio-chromedriver-service": "^8.0.0",
    "wdio-geckodriver-service": "^4.0.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "request": "^2.88.2",
    "selenium-webdriver": "^4.15.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 